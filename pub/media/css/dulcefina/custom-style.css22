  padding-left: 2px;
}
.top-menu-cart-outer ul li.top-nav-signup a:hover,.top-menu-cart-outer ul li.top-nav-signup a:focus {
    color: #af335d !important;
    text-decoration:none; 
}
.top-menu-cart-outer ul li  .minicart-wrapper {
    float: none;
    margin: 0 ;
}
.top-menu-cart-outer ul li  .minicart-wrapper a.action.viewcart:hover {
  color:#fff !important;
}

.desktop-search-btn.change-icon .search-btn:before {
  content:'\e616';
  font-family: 'icons-blank-theme';
  -webkit-font-smoothing: antialiased;
  margin: 0;
  vertical-align: top;
  display: inline-block;
  font-weight: normal;
  overflow: hidden;
  speak: none;
  font-size:40px; 
  text-align: center;
} 
.top-menu-cart-outer .minicart-wrapper .action.showcart .counter.qty {
