/* --------------------------------------------
          site search bar
---------------------------------------------- */

.nested .action.advanced {
  color: #fff;
  display: inline-block;
  font-family: "Trebuchet MS";
  font-size: 16px;
  margin-top: 8px;
  text-align: center;
}
.home-search-wrapper {
  position: absolute;
  top: -4px;
  transition: all 0.3s ease 0s;
  -webkit-transition: all 0.3s ease 0s;
  opacity:0;
  visibility:hidden;  
  right:40px;
  width:0;
}
.top-search-show.home-search-wrapper {
  width:200px;
  opacity:1;
  visibility:visible;  
    transition: all 0.3s ease 0s;
  -webkit-transition: all 0.3s ease 0s;
} 
.home-search-wrapper #search_mini_form .search button {
    height:34px;
    line-height: normal;
    width: 45px;
    background: transparent;
    border: 1px solid transparent;
    opacity: 1;
    position: absolute;
    right: 0;
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    top:-2px;
}
.home-search-wrapper  .control {
