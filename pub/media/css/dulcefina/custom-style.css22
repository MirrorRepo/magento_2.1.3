/************banner start here************/

.cms-home .banner_outer_main {
  min-height: 480px;
}

.banner_outer_main, .page_outer_main{
  background-position: center center !important;
  background-repeat: no-repeat !important;
  background-size: cover !important;
  position: relative;
}

.category_outer_main {
  min-height:165px; 
  z-index:-1; 
}

.category-inner-content {
  position: relative;
  z-index: 9;
}
.category_outer_main:before {
  background: rgba(11, 67, 62, 0.44);
  bottom: 0;
  content: "";
  left: 0;
  position: absolute;
  right: 0;
  top: 0;
}
.banner_outer_main:after, .page_outer_main:after{
    background-image: url("../images/header-shape-bottom.svg");
    bottom: -2px;
    content: "";
    height: 16px;
    left: 0;
    position: absolute;
    right: 0;
    width: 100%;
    background-repeat:repeat-x;
    background-position:center center;  
}
.banner-inner-content.page-inner-content{
  position: static;
}
.banner-inner-content {
    left: 0;
    margin: 0 auto;
    position: absolute;
    right: 0;
    text-align: center;
    bottom: 100px;
}
.discover-text {
    background: rgba(235, 161, 199, 0.76);
    color: #ffffff;
    font-family: "Oleo Script", cursive;
    font-size: 27px;
    font-weight: 800;
    float: left;
    width: 100%;
    position: relative;
    padding: 12px 0;
}
.bannet_text_price {
    float: left;
    font-size: 20px;
    width: 100%;
}
.discover-text::before {
    background: url("../images/left-banner-shape.png") no-repeat;
    content: "";
    height: 100%;
    position: absolute;
    right: 100%;
    top: 0;
    width: 28px;
}
.discover-text::after {
    background: url("../images/right-banner-shape.png") no-repeat;
    content: "";
