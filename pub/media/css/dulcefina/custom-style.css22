}
 .top-menu-cart-outer ul li .minicart-wrapper .minicart-items li strong.product-item-name {
    float: left;
    width: 100%;
    white-space: nowrap; 
    overflow: hidden;
    text-overflow: ellipsis; 
}
.top-menu-cart-outer ul li .minicart-wrapper .minicart-items li .price-container {
    float: right;
}
.top-menu-cart-outer ul li .minicart-wrapper .minicart-items li .product-item-pricing {
    float: left;
