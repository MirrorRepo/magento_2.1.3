}
.top-menu-cart-outer ul li  .block-minicart .details-qty.qty input {
    margin: 0;
}
.top-menu-cart-outer ul li .minicart-items .product-item-details {
    position: relative;
}
.top-menu-cart-outer ul li .minicart-wrapper .product .actions {
    margin: 0;
    position: absolute;
    top:32px;
    left: 134px;
    z-index: 9;
}
.top-menu-cart-outer ul li .minicart-items-wrapper {
  max-height:256px;
  overflow-x: hidden;
  overflow-y: auto;
}
aside.modal-popup.confirm._show  .modal-title {
  border:none !important;
}
.modals-overlay {
    background-color: rgba(255,255,255,0.80);
}
aside.modal-popup.confirm._show .modal-inner-wrap {
    box-shadow: 0 0 8px rgba(0, 0, 0, 0.4) !important;
    padding: 50px 10px;
    text-align: center;;
}
.modal-inner-wrap .modal-content {
    box-shadow: none ;
    border: none ;
    border-radius: 0;
}
.modal-inner-wrap  header.modal-header {
    padding: 0 ;
    border: none ;
}
.modal-inner-wrap footer.modal-footer {
    padding: 0 ;
    border: none ;
}
.modal-inner-wrap footer.modal-footer {
    text-align: center;
    margin-top: 20px !important;
}
.modal-inner-wrap  .modal-content {
    font-size: 16px;
}
.modal-inner-wrap  .action-close:before {
    color: #bf3d69 !important;
}
.modal-inner-wrap .action-secondary.action-dismiss {
    background: transparent;
    border-radius: 0;
    text-transform: uppercase;
    color:#bf3d69;
    border-color:#bf3d69;
    margin-right:3px; 
}
.modal-inner-wrap .action-secondary.action-dismiss:hover,.modal-inner-wrap .action-secondary.action-dismiss:focus {
    background:#bf3d69;
    color:#fff;
}
.modal-inner-wrap button.action-primary.action-accept {
  background:transparent;
  color:#bf3d69;
  border-color:#bf3d69;
  border-radius:0 
}

