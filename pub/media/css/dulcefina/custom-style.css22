}
.home-search-wrapper .search-autocomplete ul li:not(:empty) {
  border:none;
}
.home-search-wrapper .search-autocomplete ul li {
    background: #f5f5f5 !important;
    border-bottom: 1px solid #e9ecec !important;
    padding: 5px 40px 5px 10px;
    font-family: "Trebuchet MS";
    color: #444 !important;
    text-align: left !important;
    float: left !important;
    display: block !important;
    width: 100% !important;
    font-size: 12px;
}
.home-search-wrapper  .search-autocomplete ul li .amount {
  color: #bf3d69;
  font-size: 13px;
}
.home-search-wrapper .search-autocomplete ul {
    background: #f5f5f5;
    max-height: 322px;
    overflow-y: scroll;
