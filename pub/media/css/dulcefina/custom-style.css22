* {
    outline:none;
}
*:hover,*:focus {
    outline:none;
}
a{
  -moz-transition: all .4s;
  -ms-transition: all .4s;
  -o-transition: all .4s;
  transition: all .4s;
    color:#af335d;
}
a:hover {
  color:#af335d;
}
a:focus {
  outline:none; 
}

.navigation,
.breadcrumbs,
.page-header .header.panel,
.header.content,
.footer.content,
.page-wrapper > .widget,
.page-wrapper > .page-bottom,
.block.category.event,
.top-container,
.page-main {
    max-width: 1170px;
}
@font-face {
    font-family: 'Trebuchet MS';
    src: url('../fonts/TrebuchetMS.eot');
    src: url('../fonts/TrebuchetMS.eot?#iefix') format('embedded-opentype'), url('../fonts/TrebuchetMS.woff2') format('woff2'), url('../fonts/TrebuchetMS.woff') format('woff'), url('../fonts/TrebuchetMS.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}
/************header css start here************/

body {
    font-family: "Trebuchet MS";
}
ul li {
  list-style-type:none ;
}
legend {
  margin-left: 0 !important;
}
.action.primary {
  background: #bf3d69;
  border: 1px solid #bf3d69;
  border-radius: 0;
  color: #fff;
  font-family: "Trebuchet MS";
  font-size: 14px !important;
  line-height: 40px !important;
  padding: 0 20px !important;
  text-transform: uppercase;
  transition: all 0.25s ease-in-out 0s;
  -moz-transition: all 0.25s ease-in-out 0s;
}
ul.disc li a {
    color: #af335d;
}
.action.primary:focus, .action.primary:hover {
    background: #B32253;
    border: 1px solid #B32253;
    color: #fff;
    box-shadow: none;
}
.secondary a.action.remind {
  color: #bf3d69;
}
.navigation {
    background: #ffffff;
    top: 20px;
}
.navigation ul {
    padding: 0;
}
.nav-sections {
    background: #ffffff;
    float: left;
    margin: 0;
    padding: 20px 0 0;
    position: relative;
    z-index: 9;
}
.nav-sections .navigation li {
    float: left;
    margin: 0;
    padding: 0 11px;
}
nav ul li.search-link {
    padding: 0 !important;
}
.search-link a img {
    margin-top: -12px;
}
.logo {
    float: left;
    margin: -8px 80px 10px;
    padding: 0;
    text-align: center;
    width: 147px;
    position: relative;
    z-index:-1 ;
}
.nav-sections-item-content {
    margin: 0 auto;
    max-width: 1170px;
    padding: 0;
    width: 100%;
}
.nav-sections-item-content nav.navigation {
    float: left;
    margin: 0;
    width: calc(50% - 154px);
}
.nav-sections .navigation li {
    display: inline-block;
    float: none;
    margin: 0;
    padding: 0 11px;
}
.nav-sections-item-content nav.navigation.desktop-view ul {
    text-align: left;
}
.nav-sections-item-content nav.navigation ul {
    text-align: right;
}
.nav-sections-item-content nav.navigation ul li:first-child {
    padding-left: 0;
}
.navigation a {
    color: #888 !important;
    font-family: 'Oleo Script Swash Caps', cursive;
    font-size: 24px;
    font-weight: 400;
    font-style: italic;
}
.navigation a:hover {
    color: #af335d !important;
}
.navigation .level0 .submenu a:hover,
.navigation .level0 .submenu a.ui-state-focus {
    background: none;
}
.navigation .menu_flg {
    height: 30px;
    width: 30px;
}
.top_header_outer .panel,
.page-header .panel {
    background: none;
    border: medium none !important;
    margin: 0;
    box-shadow: none;
}
.header-wrapper .top_header_outer, .header-wrapper .page-header {
  background: #f5f5f5;
  border: medium none !important;
  float: left;
  margin: 0;
  padding:10px 0;
  position: relative;
  width: 100%;
  z-index: 99;
}
.links .welcome {
    display: none !important;
}
.header-wrapper .header .links {
    float: left !important;
}
.header .links a {
    color: #6e6e6e;
    font-family: "Trebuchet MS";
    font-size: 14px;
    font-weight: 400;
}
.nav-sections:after {
    background-image: url(../images/header-shape.svg);
    top: 100%;
    content: "";
    height: 10px;
    left: 0;
    position: absolute;
    right: 0;
    width: 100%;
    height: 16px;
    background-size: auto 100%;
    background-repeat: repeat-x;
    margin-top: -2px;
    background-position:center center;  
}
.special-outer .owl-item:nth-child(2n) .btn-read-more {
    background: #afd2d4 !important;
    border-bottom: 3px solid #65bfc4;
}
.special-outer .owl-item:nth-child(2n) .btn-read-more:hover, .special-outer .owl-item:nth-child(2n) .btn-read-more:focus {
    background: #65bfc4 !important;
    border-bottom: 3px solid #65bfc4;
}
.search-link a {
    font-size: 0 !important;
}
.social-icons-footer {
    display: none;
}
.showcart {
    display: none !important;
}
.header .showcart,
.checkout-index-index .nav-sections {
    display: block !important;
}
.page-header .links li {
    margin: 0 !important;
    padding: 5px 12px 0 0;
}
.nav-sections .navigation .submenu a {
    font-family: "Trebuchet MS";
    font-size: 14px;
    font-style: normal;
    margin: 0;
    padding: 0;
}
.submenu li {
  float: left !important;
  padding: 8px 15px !important;
  width: 156px !important;
}
.header.content .logo {
  display: none;
  float: none;
  margin: auto;
  max-width: 100%;
  position: relative;
  width: 160px;
  z-index: 9;
}
.header.content .logo img{
    margin:auto; 
}
.submenu {
    width: 470px !important;
          -moz-transition: all .4s;
    -ms-transition: all .4s;
    -o-transition: all .4s;
    transition: all .4s;
    border-top: 3px solid #bf3d69 !important;
}
.header-wrapper .header {
    padding:0 !important;
}
.search-icon.mobile-search-btn {
  display: none;
  position:absolute;
  right: 50px;
  top: 6px;
}
.custom-close-toggle-menu {
  border-radius: 100%;
  color: #f878a2;
  cursor: pointer;
  display: none;
  font-size: 22px;
  height: 25px;
  line-height: 24px;
  position: absolute;
  right: 16px;
  text-align: center;
  top: 15px;
  width: 25px;
}
.navigation .level0 .submenu a {
  display: inline-block;
}
.product_total_count {
  color: #888;
  display: inline-block;
  font-size: 12px;
  font-weight: normal;
  margin-left: 5px;
}
.submenu.sub-menu-countries-outer li {
  padding-right:0 !important 
}

/* fixed header css */

.sections.nav-sections {
      -moz-transition: all .4s;
    -ms-transition: all .4s;
    -o-transition: all .4s;
    transition: all .4s;
}
 .fixedheader .sections.nav-sections {
   position: fixed;
  z-index:999;
  margin:0;
  animation-name: menu_sticky;
  animation-duration: 0.70s;
  animation-timing-function: ease-in-out;
  height:auto;
  top:0;
  left:0;
  right:0;
}  

@keyframes menu_sticky {
 0%   {margin-top:-80px;opacity: 0;}
 50%  {margin-top: -50px;opacity: 0;}
 100% {margin-top: 0;opacity: 1;}
}
.top_header_outer .color-bar-header {
  float: left;
  width:65%;
  position: relative;
}
.top_header_outer .color-bar-header ul {
    margin: 0;
    padding: 0 0 0 5px;
    width: 100%; 
    position: absolute;
    left: 0;
    top:2px;
    visibility: hidden;
    opacity: 0;
        transition: all 0.3s ease 0s;
    -webkit-transition: all 0.3s ease 0s;
}
.top_header_outer  .top-menu-cart-outer {
  float: right;
  width: 35%;
  padding-right: 10px;
  position: relative;
}

#color-bar-button {
    cursor: pointer;
    display: block;
    float: left;
    font-size: 14px;
    margin-left: 10px;
    color: #888;
    text-transform: uppercase;
    padding-top: 18px;
    background: #f5f5f5;
    position: relative;
    z-index:9;
    min-height: 40px;
    line-height: normal;
    margin: -10px 0 0 10px;
}
#color-bar-button img {
    height: 18px;
    margin-top: -4px;
}
.top_header_outer .color-bar-header ul.swatches.open-color-bar {
  opacity:1;
  visibility:visible;
      transition: all 0.3s ease 0s;
    -webkit-transition: all 0.3s ease 0s;  
    left:195px; 
    top:2px;
}
.desktop-search-btn .search-btn:before {
    -webkit-font-smoothing: antialiased;
    font-size: 30px;
    line-height:30px;
    color: #8f8f8f;
    content: '\e615';
    font-family: 'icons-blank-theme';
    margin: 0;
    vertical-align: top;
    display: inline-block;
    font-weight: normal;
    overflow: hidden;
    speak: none;
    text-align: center;
}
.top-menu-cart-outer .minicart-wrapper .action.showcart:before {
    font-size: 30px;
    line-height:30px;
}
.top-menu-cart-outer ul {
  margin:0;
  text-align:right;  
}
.top-menu-cart-outer ul li {
    float: none;
    display:inline-block;
    margin:0; 
     transition: all 0.3s ease 0s;
    -webkit-transition: all 0.3s ease 0s;
}
.top-menu-cart-outer ul li.top-nav-signup a {
    color: #888;
    text-transform: uppercase;
    border-right: 1px solid #888;
    border-left: 1px solid #888;
    padding: 0 10px 0 10px;
    display: inline-block;
    position: relative;
    top: 4px;
    margin: 0 4px;
}
.top-menu-cart-outer ul li.user-dropdown-menu > a {
    font-size: 20px;
    color: #888;
    position: relative;
    margin-top: 0 !important;
    display: inline-block;
    position: relative;
    top: 5px;
    padding: 0 0 0 8px;
}
.top-menu-cart-outer ul li ul.myaccount-clss li a {
    color: #888;
    text-transform:capitalize;
    font-style:14px;
    padding:5px 10px;
    display:inline-block;    
}
.top-menu-cart-outer ul li a:hover {
    color: #af335d !important;
    text-decoration:none;  
}
.top-menu-cart-outer ul li a:focus {
    text-decoration:none;  
}
.top-menu-cart-outer .minicart-wrapper .action.showcart:hover:before {
    color: #af335d !important;
}
#custom-dropdown-menu {
  cursor:pointer;
}
.desktop-search-btn .search-btn:hover:before{
      color: #af335d;
}
.top-menu-cart-outer ul li  {
  position: relative;
}
.top-menu-cart-outer ul li ul.myaccount-clss {
  position: absolute;
  width:160px;
  right: 0;
  background: #fff;
  box-shadow: 0 3px 3px rgba(0,0,0,0.15);
  padding: 0;
  text-align: left;
  border:1px solid #bbb; 
  padding:8px 0; 
  margin-top:40px;
  top:0;
  z-index:100 ;
  display:none; 
}
.top-menu-cart-outer ul li ul.myaccount-clss li {
  margin:0;
  float: left;
  width:100%; 
}
.top-menu-cart-outer ul li ul.myaccount-clss:before,
.top-menu-cart-outer ul li ul.myaccount-clss:after {
    border-bottom-style: solid;
    content: '';
    display: block;
    height: 0;
    position: absolute;
    width: 0;
    top: 0;
}
.top-menu-cart-outer ul li ul.myaccount-clss:before {
    border: 6px solid;
    border-color: transparent transparent #fff transparent;
    z-index: 99;
    top: -12px;
    position: absolute;
    right: 10px;
    left: auto;
}
.top-menu-cart-outer ul li ul.myaccount-clss:after {
    border: 7px solid;
    border-color: transparent transparent #bbb transparent;
    z-index: 98;
    top: -14px;
    position: absolute;
    right: 9px;
}
.user-dropdown-menu.open-dropdown > a {
      color: #af335d !important;
}
.user-dropdown-menu.open-dropdown ul.myaccount-clss {
  display:block; 
}
.top-menu-cart-outer ul li.top-nav-signup.top-nav-register a {
  border-left:none ;
